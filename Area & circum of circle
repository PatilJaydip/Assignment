import java.util.*;

public class circle {

	public static void main(String[] args) {		Scanner sc = new Scanner(System.in);

		double r, area, circum;

		System.out.println("Enter the radiua of circle in m");

		r = sc.nextDouble();

		area = 3.142*r*r;

		circum = 2*3.142*r;

		System.out.println("Area = " +area +"sq.m");

		System.out.println("circumferance =" +circum +"m");

	}

}

OUTPUT:

Enter the radiua of circle in m

10

Area = 314.2sq.m

circumferance =62.839999999999996m
